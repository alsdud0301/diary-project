{   
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": ["./node_modules/@types","./express-session.d.ts"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    
    // "ts-node": {
    //   "esm": true
    // }, 
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/services/UserService.js", "server.js"],
  "exclude": ["node_modules"]
  ,  "proxy":"http://localhost:3000"
}
  